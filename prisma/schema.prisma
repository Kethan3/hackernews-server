generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Users {
  id       String @default(uuid())
  username String @unique
  name String?
  password  String

  createdAT DateTime @default(now())
  updatedAT DateTime  @updatedAt

}



